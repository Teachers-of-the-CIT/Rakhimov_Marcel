<Window x:Class="CosmetShop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CosmetShop"
        mc:Ignorable="d"
        Title="MainWindow" WindowState="Maximized" MinHeight="600" MinWidth="800">
    <Grid>
        <Border BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Grid.Column="1">
            <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" ></Frame>
        </Border>
    </Grid>
</Window>
